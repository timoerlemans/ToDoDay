{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "pnpm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "tododay": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/tododay",
      "sourceRoot": "projects/tododay/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/tododay",
            "index": "projects/tododay/src/index.html",
            "main": "projects/tododay/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/tododay/tsconfig.app.json",
            "assets": [
              "projects/tododay/src/favicon.ico",
              "projects/tododay/src/assets",
              "projects/tododay/src/manifest.webmanifest"
            ],
            "styles": [
              "projects/tododay/src/styles.scss"
            ],
            "scripts": [],
            "fileReplacements": [
              {
                "replace": "projects/tododay/src/environments/environment.ts",
                "with": "projects/tododay/src/environments/environment.development.ts"
              }
            ],
            "allowedCommonJsDependencies": [
              "@supabase/supabase-js"
            ],
            "buildOptimizer": true,
            "optimization": true,
            "vendorChunk": true,
            "extractLicenses": false,
            "sourceMap": true,
            "namedChunks": true,
            "stylePreprocessorOptions": {
              "includePaths": [
                "projects/tododay/src/styles"
              ]
            },
            "serviceWorker": true,
            "ngswConfigPath": "projects/tododay/ngsw-config.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/tododay/src/environments/environment.ts",
                  "with": "projects/tododay/src/environments/environment.production.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "tododay:build:production"
            },
            "development": {
              "buildTarget": "tododay:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "buildTarget": "tododay:build"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "tododay:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/tododay/tsconfig.spec.json",
            "assets": [
              "projects/tododay/src/favicon.ico",
              "projects/tododay/src/assets"
            ],
            "styles": [
              "projects/tododay/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
